{{#partial "content"}}
    <p>site: {{ site.name }}</p>
    {{#if employees}}
        {{#each employees }}
            <p>{{employeeName}} - {{ekNo}} - {{designation}}</p>
        {{/each}}
    {{else}}
        <p>No Records</p>
    {{/if}}
    <p>
        <a class="btn btn-warning btn-sm"href="/employees/{{employee.id}}/edit">edit</a>
        <a class="btn btn-warning btn-sm" href="/sites/{{employee.taskId}}/employees/{{employee.id}}/delete">delete</a>
    </p>

{{/partial}}

{{> layout.hbs}}
